    "import nbformat\n",
    "from nbformat import validate, ValidationError\n",
    "\n",
    "with open('hllsets.ipynb') as f:\n",
    "    try:\n",
    "        nb = nbformat.read(f, as_version=4)\n",
    "        validate(nb)\n",
    "        print(\"Notebook is valid\")\n",
    "    except ValidationError as e:\n",
    "        print(f\"Notebook is invalid: {e}\")"
   ]
